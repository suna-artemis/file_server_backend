{
	"name": "file-server-backend",
	"version": "0.0.1",
	"scripts": {
		"dev": "cross-env NODE_ENV=development ts-node-dev --exit-child --respawn src/index.ts",
		"start": "cross-env NODE_ENV=production ts-node src/index.ts",
		"codegen": "graphql-codegen --config codegen.ts"
	},
	"dependencies": {
		"graphql": "^16.6.0",
		"graphql-yoga": "^3.7.0"
	},
	"devDependencies": {
		"@types/node": "18.0.6",
		"cross-env": "7.0.3",
		"ts-node": "10.9.1",
		"ts-node-dev": "2.0.0",
		"typescript": "4.9.5"
	}
}